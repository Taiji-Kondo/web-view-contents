    "next": "12.2.3",
    "next-seo": "^5.5.0",
    "next-urql": "^3.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-is": "^18.2.0",
    "recoil": "^0.7.4",
    "tabler-icons-react": "^1.53.0",
    "urql": "^2.2.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.9.1",
